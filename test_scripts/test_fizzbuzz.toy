
def fizzBuzz(numberOfIterations) {
    n = 0;

    while (n < numberOfIterations) {
        if (n % 3 && n % 5) {
            print("FizzBuzz");
        } else if (n % 3) {
            print("Fizz");
        } else if (n % 5) {
            print("Buzz");
        } else {
            print(n)
        }
        n = n + 1;
    }
}

fizzBuzz(20);
